{
  "type": "module",
  "name": "@hazae41/binary",
  "version": "1.1.10",
  "description": "Zero-copy bytes reader and writer",
  "homepage": "https://github.com/hazae41/binary",
  "repository": "github:hazae41/binary",
  "author": "hazae41",
  "license": "MIT",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
